// Pedir dos n√∫meros y decir si son iguales o no. 
package pedirdosnumerosiguales;

import java.util.Scanner;

public class PedirdosNumerosIguales {

   
    public static void main(String[] args) {

        Scanner entrada = new Scanner(System.in);
        
        int numero1, numero2;
        
        System.out.println("Ingrese numero 1");
        numero1 = entrada.nextInt();
        
        System.out.println("Ingrese numer 2");
        numero2 = entrada.nextInt();
        
        if (numero1 == numero2){
            System.out.println("Los numeros son iguales" + numero1 + numero2);
        }
            else 
                    System.out.println("Los numero no son iguales" + numero1 + numero2);           
        }
    }
